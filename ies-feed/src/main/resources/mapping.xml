<beanio xmlns="http://www.beanio.org/2012/03" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.beanio.org/2012/03 http://www.beanio.org/2012/03/mapping.xsd">

  <stream name="reversalFile" format="delimited">
   <parser>
      <property name="delimiter" value="|" />
    </parser>
    <record name="primaryOrderInfo" class="com.modetransportation.batch.model.flatfile.PrimaryOrderInfo">
      <field name="recordType" />
      <field name="initialFeedFlag" />
      <field name="delta7Number" />
      <field name="officeCode" />
    </record>
  </stream>
</beanio>